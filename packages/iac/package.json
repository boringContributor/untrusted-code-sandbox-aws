{
  "name": "@untrusted-code/iac",
  "version": "1.0.0",
  "description": "AWS CDK Infrastructure for untrusted JavaScript code executor",
  "scripts": {
    "build": "tsc --noEmit",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "npm run build && npm run build:lambda && cdk deploy",
    "build:lambda": "cd ../lambda && cargo lambda build --release --arm64",
    "synth": "npm run build && cdk synth",
    "diff": "npm run build && cdk diff",
    "destroy": "cdk destroy"
  },
  "keywords": ["aws", "cdk", "lambda", "infrastructure"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.103.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.232.1",
    "constructs": "^10.4.3"
  }
}
